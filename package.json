{
  "name": "hiddenwisdom-react-js",
  "description": "hidden wisdom front end proj.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lagos-devs/hiddenwisdom-react-js.git"
  },
  "scripts": {
    "start": "npm-run-all --parallel open:app lint:watch",
    "prod": "NODE_ENV=production node app/tools/distServer.js",
    "open:app": "node server.js",
    "test": "mocha --reporter progress app/tools/testSetup.js \"app/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "rimraf coverage/ && istanbul cover _mocha app/tools/testSetup.js \"app/**/*.spec.js\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "node_modules/.bin/esw webpack.config.* app tools",
    "lint:watch": "npm run lint -- --watch",
    "build:html": "node app/tools/buildHtml.js",
    "prebuild": "npm-run-all test lint build:html",
    "build": "webpack --config ./webpack.production.config.js --progress --colors"
  },
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.11.6",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1"
  },
  "devDependencies": {
    "colors": "^1.1.2",
    "coveralls": "^2.11.12",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-react": "^5.0.1",
    "eslint-watch": "^2.1.11",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.19.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^8.5.0",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "npm-run-all": "^1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "bugs": {
    "url": "https://github.com/lagos-devs/hiddenwisdom-react-js/issues"
  },
  "homepage": "https://github.com/lagos-devs/hiddenwisdom-react-js#readme"
}
